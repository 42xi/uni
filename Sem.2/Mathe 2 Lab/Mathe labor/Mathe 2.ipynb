    "from scipy import integrate\n",
    "import numpy as np\n",
    "\n",
    "# Definiere die Funktion\n",
    "def f(x):\n",
    "    return np.exp(-x**2)\n",
    "\n",
    "# Berechne das bestimmte Integral von 0 bis 0.25\n",
    "result, error = integrate.quad(f, 0, 0.25)\n",
    "\n",
    "# Gib das Ergebnis aus\n",
    "print(\"Näherungswert des Integrals:\", result)\n"
   ]
  }
